php_value max_input_vars 100000
<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteRule    ^$    public/    [L]
    RewriteRule    (.*) public/$1    [L]

    RewriteRule    ^$    blog/    [L]
    RewriteRule    (.*) blog/$1    [L]

    RewriteRule    ^$    transferadmin/    [L]
    RewriteRule    (.*) transferadmin/$1    [L]
    
 </IfModule>
